{
  "name": "cotacao-back",
  "version": "1.0.0",
  "description": "quotation api rest express typeorm orm ",
  "main": "index.js",
  "scripts": {
    "########################################## SERVER ##############################################": "",
    "start": "node dist/index.js",
    "start:ts": "NODE_ENV=prod ts-node -r tsconfig-paths/register --transpile-only src/index.ts",
    "dev": "npx ts-node-dev -r tsconfig-paths/register --transpile-only src/index.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "buildbabel": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files ",
    "########################################## DOCKER ##############################################": "",
    "docker:up": "docker compose up",
    "docker:down": "docker compose down",
    "########################################## TYPEORM #############################################": "",
    "create:migrate": "NODE_ENV=dev typeorm migration:create -n",
    "migrate:local": "NODE_ENV=dev ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "revert:local": "NODE_ENV=dev ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert",
    "migrate": "typeorm migration:run",
    "revert": "typeorm migration:revert",
    "########################################## FORMATTER ###########################################": "",
    "eslint": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "quotation",
    "api",
    "rest",
    "express",
    "typeorm",
    "orm"
  ],
  "author": "infinity byte",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.9.6",
    "@types/nodemailer": "^6.4.4",
    "@types/socket.io": "^3.0.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.4.3"
  }
}
